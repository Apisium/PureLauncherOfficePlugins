{
  "name": "pure-launcher-official-plugins",
  "version": "0.0.0",
  "private": true,
  "description": "The official plugins of PureLauncher.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint packages/ --ext .js,.ts,.tsx",
    "dev:mi": "pl-plugin-dev -e packages/MultiInstances/index.tsx",
    "build:mi": "pl-plugin-dev build -e packages/MultiInstances/index.tsx",
    "dev:ai": "pl-plugin-dev -e packages/AuthlibInjector/index.ts",
    "build:ai": "pl-plugin-dev build -e packages/AuthlibInjector/index.ts",
    "dev:bot": "pl-plugin-dev -e packages/MCBot/index.tsx",
    "build:bot": "pl-plugin-dev build -e packages/MCBot/index.tsx",
    "dev:cf": "pl-plugin-dev -e packages/CurseForge/index.tsx",
    "build:cf": "pl-plugin-dev build -e packages/CurseForge/index.tsx",
    "dev:skins": "pl-plugin-dev -e packages/Skins/index.tsx",
    "build:skins": "pl-plugin-dev build -e packages/Skins/index.tsx",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apisium/PureLauncherOfficialPlugins.git"
  },
  "keywords": [
    "PureLauncher",
    "Minecraft",
    "TypeScript",
    "Plugins"
  ],
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apisium/PureLauncherOfficialPlugins/issues"
  },
  "homepage": "https://github.com/Apisium/PureLauncherOfficialPlugins#readme",
  "dependencies": {
    "@xmcl/task": "^2.1.4",
    "@xmcl/text-component": "^2.1.1",
    "cloudscraper": "^4.6.0",
    "commander": "^5.0.0",
    "crc": "^3.8.0",
    "crc32-stream": "^3.0.1",
    "filenamify": "^4.1.0",
    "fs-extra": "^8.1.0",
    "history": "^4.10.1",
    "minecraft-data": "^2.41.0",
    "minecraft-protocol": "^1.11.0",
    "pure-launcher": "git+https://github.com/Apisium/PureLauncher.git",
    "react-router-cache-route": "^1.8.4",
    "request": "^2.88.2",
    "reqwq": "^0.2.4",
    "semver": "^7.1.3",
    "yazl": "^2.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@xmcl/core": "^2.0.7",
    "@xmcl/installer": "^2.6.0",
    "@xmcl/unzip": "^1.1.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "electron": "^8.1.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
